{
    "name": "color-elephant/coffective-api",
    "type": "project",
    "description": "Ð¡offective API",
    "license": "MIT",
    "require": {
        "php": "^7.2.12",
        "fico7489/laravel-pivot": "^3.0",
        "fideloper/proxy": "^4.1",
        "genealabs/laravel-model-caching": "^0.4.9",
        "jenssegers/mongodb": "^3.5",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.2",
        "laravel/scout": "^7.1",
        "laravel/tinker": "^1.0",
        "mikemclin/laravel-wp-password": "^2.0",
        "pragmarx/google2fa": "^5.0",
        "predis/predis": "^1.1",
        "s-ichikawa/laravel-sendgrid-driver": "^2.0",
        "santigarcor/laratrust": "^5.2",
        "teamtnt/laravel-scout-tntsearch-driver": "^7.0"
    },
    "require-dev": {
        "arcanedev/log-viewer": "^4.7",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.2",
        "codedungeon/phpunit-result-printer": "^0.26.1",
        "darkaonline/l5-swagger": "^5.8",
        "doctrine/dbal": "^2.9",
        "filp/whoops": "^2.3",
        "fzaninotto/faker": "^1.8",
        "krlove/eloquent-model-generator": "^1.3",
        "laravel/telescope": "^2.0",
        "mockery/mockery": "^1.2",
        "moontoast/math": "^1.1",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    }
}
